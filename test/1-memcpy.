#include "main.h"
/**
 * _memcpy - copies memory area
 * @dest: destination memory area
 * @src: source memory area
 * @n: number of bytes from n to copy
 * Return: pointer to dest
 */

char *_memcpy(char *dest, char *src, unsigned int n)
{
	int i;
	int j:

	for (; i < n; i++)
	{
		dest[i] = src[i];
		n--;
	}

	return (dest);
}

